<%= render 'books/template', user: @user %>

<h2>Users</h2>
<table>
  <% @users.each do |user| %>
 <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td><%= image_tag user.get_profile_image(100, 100) %></td>
      <td><%= user.name %></td>
      <td>
        <%= link_to user_path(user.id) do %>
        show
        <% end %>
      </td>
    </tr>
  </tbody>
  <% end %>
</table>