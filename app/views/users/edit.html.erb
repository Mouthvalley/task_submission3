<h2>User info</h2>

<%= form_with model:@user do |f| %>
  <div for="Name">Name</div>
  <%= f.text_field :name, autofocus: true, id:"Name", placeholder:"Name"%>

  <div for="Image">Image</div>
  <%= f.file_field :profile_image, placeholder: "プロフィール画像", accept: "image/*" %>

  <%= f.submit "Update User" %>
<% end %>