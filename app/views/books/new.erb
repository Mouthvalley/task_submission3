<!--部分テンプレート-->
<h1>User info</h1>
<h4>画像</h4>


<h1>New book</h1>
<!-- form_with部分 -->
<%= form_with model: Book.new do |f| %>
  <h4>画像</h4>
  　<%= f.file_field :profile_image, accept: "image/*" %>
  <h4>Title</h4>
  　<%= f.text_field :shop_name %>

  <h4>Opinion</h4>
  　<%= f.text_area :caption %>

  <%= f.submit 'Create Book' %>
<% end %>